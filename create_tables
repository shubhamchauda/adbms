create table LOCATION (Location_ID int(20) not null, Regional_Group varchar(30),primary key(Location_ID));
create table DEPARTMENT (Department_ID int (20) not null, Name varchar(30), Location_ID int(20),primary key(Department_ID));
 create table JOB (Job_ID int(10) not null, Functionn varchar(20),primary key(Job_ID));

create table EMPLOYEE (EMPLOYEE_ID int(30), LAST_NAME varchar(20), FIRST_NAME varchar(20),
MIDDLE_NAME varchar(10), JOB_ID int(30), MANAGER_ID int(50), HIREDATE date, SALARY int(50),
COMM int(50),DEPARTMENT_ID int(20));
